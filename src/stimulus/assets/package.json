        "@hotwired/stimulus": "^3.0.0",
        "@simplewebauthn/browser": "^8.0.0"
    },
    "devDependencies": {
        "@hotwired/stimulus": "^3.0.0",
        "@simplewebauthn/browser": "^8.0.0"
    }
