{
    "name": "web-auth/webauthn-symfony-bundle",
    "type": "symfony-bundle",
    "license": "MIT",
    "description": "FIDO2/Webauthn Security Bundle For Symfony",
    "keywords": [
        "FIDO",
        "FIDO2",
        "webauthn",
        "symfony",
        "symfony-bundle",
        "bundle"
    ],
    "homepage": "https://github.com/web-auth",
    "authors": [
        {
            "name": "Florent Morselli",
            "homepage": "https://github.com/Spomky"
        },
        {
            "name": "All contributors",
            "homepage": "https://github.com/web-auth/webauthn-symfony-bundle/contributors"
        }
    ],
    "require": {
        "php": ">=8.3",
        "psr/event-dispatcher": "^1.0",
        "symfony/config": "^7.0",
        "symfony/dependency-injection": "^7.0",
        "symfony/framework-bundle": "^7.0",
        "symfony/http-client": "^7.0",
        "symfony/security-bundle": "^7.0",
        "symfony/security-core": "^7.0",
        "symfony/security-http": "^7.0",
        "symfony/serializer": "^7.0",
        "symfony/validator": "^7.0",
        "web-auth/webauthn-lib": "self.version"
    },
    "extra": {
        "thanks": {
            "name": "web-auth/webauthn-framework",
            "url": "https://github.com/web-auth/webauthn-framework"
        }
    },
    "autoload": {
        "psr-4": {
            "Webauthn\\Bundle\\": "src/"
        }
    }
}
