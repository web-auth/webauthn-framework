{
    "name": "web-auth/webauthn-symfony-bundle",
    "type": "symfony-bundle",
    "license": "MIT",
    "description": "FIDO2/Webauthn Security Bundle For Symfony",
    "keywords": ["FIDO", "FIDO2", "U2F", "webauthn"],
    "homepage": "https://github.com/web-auth",
    "authors": [
        {
            "name": "Florent Morselli",
            "homepage": "https://github.com/Spomky"
        },
        {
            "name": "All contributors",
            "homepage": "https://github.com/web-auth/webauthn-symfony-bundle/contributors"
        }
    ],
    "require": {
        "php-http/httplug-bundle": "^1.0",
        "spomky-labs/cbor-bundle": "^1.0",
        "spomky-labs/cbor-php": "^1.0.2",
        "symfony/config": "^4.2",
        "symfony/dependency-injection": "^4.2",
        "symfony/framework-bundle": "^4.2",
        "web-auth/webauthn-lib": "^1.1",
        "web-token/jwt-signature": "^1.3"
    },
    "autoload": {
        "psr-4": {
            "Webauthn\\Bundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Webauthn\\Bundle\\Tests\\Functional\\": "tests/functional/"
        }
    },
    "suggest": {
        "web-auth/webauthn-symfony-security-bundle": "For firewall features",
        "php-http/httplug-bundle": "For Android SafetyNet support on a Symfony application (highly recommended)"
    },
    "extra": {
        "branch-alias": {
            "v1.0": "1.0.x-dev",
            "v1.1": "1.1.x-dev"
        }
    }
}
