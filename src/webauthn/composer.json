{
    "name": "web-auth/webauthn-lib",
    "type": "library",
    "license": "MIT",
    "description": "FIDO2/Webauthn Support For PHP",
    "keywords": ["FIDO", "FIDO2", "U2F", "webauthn"],
    "homepage": "https://github.com/web-auth",
    "authors": [
        {
            "name": "Florent Morselli",
            "homepage": "https://github.com/Spomky"
        },
        {
            "name": "All contributors",
            "homepage": "https://github.com/web-auth/webauthn-library/contributors"
        }
    ],
    "require": {
        "php": "^7.1",
        "ext-json": "*",
        "ext-openssl": "*",
        "ext-mbstring": "*",
        "php-http/httplug": "^1.0",
        "psr/http-message": "^1.0",
        "spomky-labs/base64url": "^2.0",
        "spomky-labs/cbor-php": "^1.0.2",
        "thecodingmachine/safe": "^0.1",
        "beberlei/assert": "^3.0",
        "web-auth/cose-lib": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Webauthn\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Webauthn\\Tests\\Functional\\": "tests/functional/",
            "Webauthn\\Tests\\Unit\\": "tests/unit/"
        }
    },
    "suggest": {
        "php-http/httplug": "For Android SafetyNet support (highly recommended)"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    }
}
